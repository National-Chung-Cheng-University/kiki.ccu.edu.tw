#!/bin/csh

echo "create directory and symbolic links"
mkdir /ultra2 /home0
ln -s /NFS/project /ultra2/
ln -s /NFS/project /home0/
tar zxvfp apache22.httpd.Includes.tgz -C /usr/local/etc/apache22/Includes/
